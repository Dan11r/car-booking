{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "es5",
    "allowJs": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@store": ["src/store"],
      "@store/*": ["src/store","src/store/*"],
      "@ui/*": ["src/components/ui/*", "src/components/ui"],
      "@interface/*": ["src/interface/*", "src/interface"],
      "@hooks/*": ["src/hooks/*", "src/hooks"],
      "@hooks": ["src/hooks"],
      "@images": ["src/assets/images"],
      "@images/*": ["src/assets/images/*", "src/assets/images"],
    }
  },
  "include": ["./src/**/*"],
  "exclude": [
    "node_modules"
  ]
}
